    "axios": "^0.21.1",
    "core-js": "^3.6.5",
    "echarts": "^4.9.0",
    "insert-css": "^2.0.0",
    "js-cookie": "^2.2.1",
    "less": "^3.11.1",
    "less-loader": "^6.1.2",
    "nprogress": "^0.2.0",
    "style-resources-loader": "^1.4.1",
    "vue": "^2.6.11",
